{
  "name": "vitepress-code-preview",
  "version": "1.0.0",
  "description": "preview component of code and component in vitepress",
  "keywords": [
    "Vite",
    "VitePress",
    "Vue",
    "JSX",
    "TSX",
    "demo",
    "preview"
  ],
  "author": "welives",
  "license": "MIT",
  "homepage": "https://github.com/welives/vitepress-code-preview",
  "repository": {
    "type": "git",
    "url": "git@github.com:welives/vitepress-code-preview.git"
  },
  "workspaces": [
    "docs",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "docs:dev": "pnpm --filter=code-preview-example dev",
    "docs:preview": "pnpm --filter=code-preview-example preview",
    "docs:build": "pnpm --filter=code-preview-example build",
    "build": "pnpm container:build && pnpm plugin:build",
    "container:build": "pnpm --filter=@vitepress-code-preview/container build",
    "plugin:build": "pnpm --filter=@vitepress-code-preview/plugin build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vue": "^3.3.8"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">= 16"
  }
}
